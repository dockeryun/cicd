#以下为配置修改
#用于镜像名称
project_name="diy2015_wordpress_web"
#提供主要容器名(基于dockerfile镜像启动的容器）即.yaml文件中容器名
primary_container_name="diy2015_wordpress_web"
#提供项目中所有启动容器列表
container_names="diy2015_wordpress_web"
#docker仓库
docker_registry="183.238.173.78:6000"
#auth是否docker仓库有设置密码，yes有，no无
auth=yes
#user是docker仓库用户名
user=test
#pass是docker仓库密码
pass=123456
#预正式serverprod部署的服务器别名
serverprod=prod
#测试serverdev部署的服务器别名
serverdev=test
